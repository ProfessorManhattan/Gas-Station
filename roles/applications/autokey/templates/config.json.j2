{
    "type": "script",
    "description": "{{ autokey_config.name }}",
    "store": {},
    "modes": [
        3
    ],
    "usageCount": 0,
    "prompt": false,
    "omitTrigger": false,
    "showInTrayMenu": false,
    "abbreviation": {
        "abbreviations": [],
        "backspace": true,
        "ignoreCase": false,
        "immediate": false,
        "triggerInside": false,
        "wordChars": "[\\w]"
    },
    "hotkey": {
        "modifiers": [
            "<alt>",
            "<ctrl>"
        ],
        "hotKey": "{{ autokey_config.hotkey }}"
    },
    "filter": {
        "regex": null,
        "isRecursive": false
    }
}
